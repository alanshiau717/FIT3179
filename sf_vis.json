{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "width": 800,
    "height": 500,
    "title": "Crime In San Francisco",
    "projection": {"type": "albersUsa"},
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/alanshiau717/FIT3179/main/SanFrancisco.Neighborhoods%20(1).json",
          "format": {"type": "topojson", "feature": "SanFrancisco"}
        },
        "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "black"}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/alanshiau717/FIT3179/main/Police_Department_Incidents_-_Previous_Year__2016_.csv"
        },
         "projection": {
          "type": "albersUsa"
        },
        "encoding": {
          "longitude": {"field": "X", "type": "quantitative"},
          "latitude": {"field": "Y", "type": "quantitative"}
        },
        "mark": "circle",
        "size": {"value": 10}
      }
      
    ]
  }